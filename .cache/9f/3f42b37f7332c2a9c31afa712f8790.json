ted":{"line":103,"column":197},"source":"../src/components/Block.js","original":{"line":54,"column":21}},{"name":"displayTransaction","generated":{"line":103,"column":202},"source":"../src/components/Block.js","original":{"line":54,"column":26}},{"generated":{"line":103,"column":220},"source":"../src/components/Block.js","original":{"line":50,"column":16}},{"generated":{"line":103,"column":221},"source":"../src/components/Block.js","original":{"line":49,"column":12}},{"generated":{"line":103,"column":222},"source":"../src/components/Block.js","original":{"line":48,"column":8}},{"generated":{"line":104,"column":0},"source":"../src/components/Block.js","original":{"line":60,"column":5}},{"name":"Component","generated":{"line":108,"column":2},"source":"../src/components/Block.js","original":{"line":4,"column":20}},{"name":"Block","generated":{"line":110,"column":15},"source":"../src/components/Block.js","original":{"line":62,"column":15}}],"sources":{"../src/components/Block.js":"import React, { Component } from \"react\";\nimport { Button } from 'react-bootstrap'\nimport Transaction from \"./Transaction\";\nclass Block extends Component {\n    state = { displayTransaction: false }\n\n    toggleTransaction = () => {\n        this.setState({ displayTransaction: !this.state.displayTransaction })\n    }\n\n    get displayTransaction() {\n        const { data } = this.props.block;\n        const stringifiedData = JSON.stringify(data)\n\n        const dataDisplay = stringifiedData.length > 35 ? `${stringifiedData.substring(0, 35)}...` : stringifiedData;\n\n        if (this.state.displayTransaction) {\n            return (\n                <div>\n                    {\n                        data.map(transaction => (\n                            <div key={transaction.id}>\n                                <hr />\n                                <Transaction transaction={transaction} />\n                            </div>\n                        ))\n                    }\n                    <br />\n                    <Button variant=\"outline-danger\" size=\"sm\" onClick={this.toggleTransaction}>Show Lass</Button>\n                </div>\n            )\n        }\n        return (\n            <div>\n                <div>Data : {dataDisplay}</div>\n                <Button variant=\"outline-danger\" size=\"sm\" onClick={this.toggleTransaction}>Show More</Button>\n            </div>\n        )\n    }\n    render() {\n        const { timestamp, hash } = this.props.block;\n\n\n\n        const hashDisplay = `${hash.substring(0, 15)}...`\n\n\n        return (\n            <div className='block'>\n                <div className=\"blos\">\n                    <div>Hash: {hashDisplay}</div>\n                    <div>Timestamp: {new Date(timestamp).toLocaleString()}</div>\n\n                    {this.displayTransaction}\n\n                </div>\n\n            </div>\n        )\n    }\n}\nexport default Block;"},"lineCount":null}},"error":null,"hash":"e35d1adff626aaddc56f67f71fe2fc8b","cacheData":{"env":{}}}